{% extends '::base.html.twig' %}

{% block title %}Bisouland v2 - Personnages{% endblock %}

{% block body %}
    <h1>Personnages</h1>

    <table>
        <tr>
            <th>Nom</th>
            <th>Date de naissance</th>
        </tr>

        {% for being in beings %}
            <tr>
                <td>{{ being.name }}</td>
                <td>{{ being.created|date("h:m:s d/m/Y") }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
